      <AnalyzerSettings>
        <CollectionProperty Name="Hungarian">
          <Value>cc</Value>
          <Value>ip</Value>
          <Value>iv</Value>
          <Value>nt</Value>
          <Value>db</Value>
		  <Value>hp</Value>
        </CollectionProperty>
